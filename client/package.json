{
  "name": "RemoteDevClient",
  "version": "1.0.0",
  "description": "Mobile client for remote server development with Claude Code integration",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:build:ios:release": "detox build --configuration ios.sim.release",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:test:ios:release": "detox test --configuration ios.sim.release",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:build:android:release": "detox build --configuration android.emu.release",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:test:android:release": "detox test --configuration android.emu.release"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/__tests__/**/*.test.tsx"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/__tests__/**"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|@react-navigation|react-native-keychain)/)"
    ],
    "moduleNameMapper": {
      "^react-native$": "<rootDir>/node_modules/react-native"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-community/netinfo": "^11.5.1",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.12.0",
    "@react-navigation/stack": "^7.6.16",
    "react": "^19.0.0",
    "react-native": "0.83.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-keychain": "^10.0.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.20.0",
    "react-native-webview": "^13.16.0",
    "uuid": "^13.0.0",
    "zustand": "^5.0.11"
  },
  "overrides": {
    "fast-xml-parser": "^5.3.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-flow": "^7.27.1",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@react-native-community/cli": "^20.1.1",
    "@react-native/metro-config": "^0.83.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@tsconfig/react-native": "^3.0.9",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.10",
    "@types/react-native": "^0.72.8",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^30.2.0",
    "detox": "^20.0.0",
    "jest": "^30.2.0",
    "jest-circus": "^30.2.0",
    "react-test-renderer": "^19.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.9.3"
  }
}
